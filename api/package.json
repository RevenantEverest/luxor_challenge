{
  "name": "api",
  "module": "server.js",
  "scripts": {
    "build": "swc src -d dist",
    "build-dev": "bun run build && NODE_ENV=development node --inspect dist/server.js",
    "dev": "nodemon -L --ext ts --exec \"bun run build-dev\"",
    "start": "bun run build && NODE_ENV=production node dist/server.js",
    "test": "NODE_ENV=test jest --setupFiles dotenv/config --runInBand",
    "lint": "eslint . --ext ts --report-unused-disable-directives --max-warnings 0",
    "seed": "bun run build && node -e \"import('./dist/db/seeding.js').then( loadedModule => loadedModule.default() )\"",
    "reset:npm": "rm -rf node_modules && bun install"
  },
  "type": "module",
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "@faker-js/faker": "^8.4.1",
    "bcrypt": "^5.1.1",
    "chalk": "^5.3.0",
    "cors": "^2.8.5",
    "dayjs": "^1.11.10",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "pg": "^8.11.5",
    "reflect-metadata": "^0.2.2",
    "typeorm": "^0.3.20",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@swc/cli": "^0.1.62",
    "@swc/core": "1.3.75",
    "@swc/jest": "^0.2.29",
    "@swc/types": "^0.1.5",
    "@types/bcrypt": "^5.0.2",
    "@types/bun": "latest",
    "@types/chalk": "^2.2.0",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.12.7",
    "@types/pg": "^8.11.5",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^7.7.0",
    "@typescript-eslint/parser": "^7.7.0",
    "bun-types": "^1.1.3",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.0",
    "supertest": "^6.3.4",
    "ts-jest": "^29.1.2",
    "ts-jest-resolver": "^2.0.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5"
  }
}